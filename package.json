{
  "name": "beyond-rgb",
  "productName": "Beyond RGB",
  "version": "0.0.1",
  "description": "Spectral image calibration on a dime",
  "main": "src/main/index.js",
  "scripts": {
    "dev": "concurrently -k \"npm:svelte-dev\" \"npm:electron-dev\"",
    "package": "npm run svelte-build && electron-forge package",
    "build": "npm run svelte-build && electron-forge make",
    "publish": "npm run svelte-build && electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "preview": "npm run svelte-build && electron-forge start",
    "electron-dev": "cross-env ELEC_ENV=dev electron-forge start",
    "svelte-build": "vite build",
    "svelte-dev": "vite",
    "svelte-start": "vite preview"
  },
  "keywords": [],
  "author": {
    "name": "pxd5295",
    "email": "pxd5295@rit.edu"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "BeyondRGB"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "autoprefixer": "^10.3.7",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "electron": "15.3.0",
    "postcss": "^8.3.11",
    "postcss-load-config": "^3.1.0",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.44.0",
    "svelte-awesome": "^2.4.2",
    "svelte-preprocess": "^4.9.8",
    "tailwindcss": "^2.2.17",
    "vite": "^2.6.11"
  }
}
